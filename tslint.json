{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "curly": false,
        "interface-name": false,
        "no-console": {
            "severity": "warn"
        },
        "no-bitwise": false,
        "no-string-literal": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "radix": false
    }
}